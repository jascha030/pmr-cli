{
  "name": "jascha030/pmr-cli",
  "description": "Small CLI Utility for PM purposes.",
  "require": {
    "php": "^7.4|^8.0",
    "symfony/console": "^5.3",
    "symfony/process": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.2",
    "composer/composer": "^2.7",
    "roave/security-advisories": "dev-latest"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Jascha030\\PM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jascha030\\PM\\Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jascha030",
      "email": "contact@jaschavanaalst.nl"
    }
  ],
  "minimum-stability": "stable",
  "bin": [
    "bin/pmr"
  ],
  "scripts": {
    "phpunit": "vendor/bin/phpunit"
  }
}
